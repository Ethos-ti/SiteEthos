{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "hacklabr/read-more",
    "title": "Read more",
    "description": "A collapsible text paragraph with 'Read more' button",
    "category": "text",
    "icon": "editor-insertmore",
    "supports": {
        "html": false
    },
    "attributes": {
        "lines": {
            "type": "number",
            "default": 3
        }
    },
    "textdomain": "hacklabr",
    "editorScript": ["file:../../../dist/blocks/read-more/editor.js"],
    "editorStyle": ["file:../../../dist/blocks/read-more/editor.css"],
    "script": ["file:../../../dist/blocks/read-more/index.js"],
    "style": ["file:../../../dist/blocks/read-more/index.css"]
}
